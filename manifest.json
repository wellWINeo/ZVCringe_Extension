{
    "name": "ZCringe_Extension",
    "description": "Replaces з/З to Z and в/В to V",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}